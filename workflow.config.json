{
  "workflow_description": {
    "name":"raw_fastq_qc",
    "version": 1.0,
    "label":"Raw fastq QC",
    "type":"raw_fastq_qc",
    "inputs":"raw_fastq/{sample}*fastq.gz",
    "outputs":[
      "qc_reports/raw_fastq_multiqc.html",
      "qc_reports/{sample}/raw_fastqc/*"
    ],
    "report_index": "qc_reports/raw_fastq_multiqc.html",
    "reports":[
      "qc_reports/raw_fastq_multiqc.html"
    ]
  },
  "general_params": [
    "library_name",
    "sample_name",
    "run_forward_read_length",
    "run_reverse_read_length",
    "lib_forward_read_length",
    "lib_reverse_read_length"
  ]
}